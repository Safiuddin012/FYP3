<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="screenThreeVM" type="com.safiuddinsapplication.app.modules.screenthree.data.viewmodel.ScreenThreeVM"/>
    </data>
    <LinearLayout android:gravity="start"
        android:id="@+id/linearColumngroupone"
        android:layout_height="match_parent"
        android:layout_marginBottom="@dimen/_25pxv"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStyleblack_900">
        <FrameLayout android:id="@+id/frameStacklanguage"
            android:layout_height="@dimen/_206pxv" android:layout_width="match_parent">
            <FrameLayout android:id="@+id/frameStackgroupone"
                android:layout_gravity="top|start"
                android:layout_height="@dimen/_175pxv"
                android:layout_marginBottom="@dimen/_10pxv" android:layout_width="match_parent">
                <ImageView android:id="@+id/imageGroupOne"
                    android:layout_gravity="start"
                    android:layout_height="@dimen/_175pxh"
                    android:layout_width="match_parent"
                    android:scaleType="fitXY"
                    imageRes="@{@drawable/img_group1}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_group1"/>
                <androidx.appcompat.widget.Toolbar
                    android:id="@+id/toolbarToolbar"
                    android:layout_gravity="top|center"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_54pxv"
                    android:layout_marginEnd="@dimen/_42pxh"
                    android:layout_marginStart="@dimen/_42pxh"
                    android:layout_marginTop="@dimen/_54pxv"
                    android:layout_width="match_parent" app:contentInsetStart="0dp">
                    <LinearLayout
                        android:id="@+id/linearRowpracticesessio"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtPracticeSessio"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_20pxv"
                            android:layout_weight="0.85"
                            android:layout_width="0dp"
                            android:singleLine="true"
                            android:text="@{screenThreeVM.screenThreeModel.txtPracticeSessio}"
                            style="@style/txtABeeZeeitalic32" tools:text="@string/msg_practice_sessio"/>
                        <ImageView
                            android:id="@+id/imageEllipseSeventyNine"
                            android:layout_height="@dimen/_40pxh"
                            android:layout_marginBottom="@dimen/_18pxv"
                            android:layout_marginStart="@dimen/_26pxh"
                            android:layout_width="@dimen/_40pxh"
                            android:scaleType="centerCrop"
                            circular="@{true}"
                            imageRes="@{@drawable/img_ellipse78}"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_ellipse78"/>
                    </LinearLayout>
                </androidx.appcompat.widget.Toolbar>
            </FrameLayout>
            <LinearLayout android:id="@+id/linearDropdownContai"
                android:layout_gravity="bottom|start"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_24pxh"
                android:layout_marginStart="@dimen/_24pxh"
                android:layout_marginTop="@dimen/_10pxv"
                android:layout_width="wrap_content"
                android:orientation="vertical" style="@style/groupStylecornerRadius">
                <Spinner
                    android:background="@drawable/layer_list_bg_white_a700_border_black_900_19_radius_18"
                    android:gravity="start"
                    android:id="@+id/spinnerLanguage"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" tools:listitem="@layout/spinner_item"/>
            </LinearLayout>
        </FrameLayout>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearScreenThree"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_19pxh"
            android:layout_marginStart="@dimen/_19pxh"
            android:layout_marginTop="@dimen/_44pxv"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStyleblack_900">
            <LinearLayout android:gravity="center_horizontal"
                android:id="@+id/linearDropdownContai1"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginStart="@dimen/_5pxh"
                android:layout_width="wrap_content"
                android:orientation="vertical" style="@style/groupStylecornerRadius">
                <Spinner
                    android:background="@drawable/layer_list_bg_white_a700_border_black_900_19_radius_18"
                    android:gravity="start"
                    android:id="@+id/spinnerButton"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" tools:listitem="@layout/spinner_item"/>
            </LinearLayout>
            <androidx.appcompat.widget.AppCompatButton
                android:ellipsize="end" android:gravity="center"
                android:id="@+id/btnConnectTheMachine"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_75pxv"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_23pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingStart="@dimen/_30pxh"
                android:paddingTop="@dimen/_23pxv"
                android:singleLine="true"
                android:text="@string/msg_connect_the_mac"
                android:textAllCaps="false"
                style="@style/btnSolidRounded_1" tools:text="@string/msg_connect_the_mac"/>
            <androidx.appcompat.widget.AppCompatButton
                android:ellipsize="end" android:gravity="center"
                android:id="@+id/btnConnectTheBatOne"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_42pxv"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_23pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingStart="@dimen/_30pxh"
                android:paddingTop="@dimen/_23pxv"
                android:singleLine="true"
                android:text="@string/lbl_connect_the_bat"
                android:textAllCaps="false"
                style="@style/btnSolidRounded_1" tools:text="@string/lbl_connect_the_bat"/>
            <androidx.appcompat.widget.AppCompatButton
                android:ellipsize="end" android:gravity="center"
                android:id="@+id/btnNext"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_42pxv"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_24pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingStart="@dimen/_30pxh"
                android:paddingTop="@dimen/_24pxv"
                android:singleLine="true"
                android:text="@string/lbl_next"
                android:textAllCaps="false"
                style="@style/btnSolidRounded_1" tools:text="@string/lbl_next"/>
            <FrameLayout android:id="@+id/frameStack"
                android:layout_height="@dimen/_56pxv"
                android:layout_marginEnd="@dimen/_15pxh"
                android:layout_marginStart="@dimen/_15pxh"
                android:layout_marginTop="@dimen/_93pxv"
                android:layout_width="@dimen/_56pxh" style="@style/groupStylelime_1">
                <FrameLayout android:id="@+id/frameHome"
                    android:layout_gravity="center"
                    android:layout_height="@dimen/_28pxv"
                    android:layout_marginBottom="@dimen/_14pxv"
                    android:layout_marginEnd="@dimen/_11pxh"
                    android:layout_marginStart="@dimen/_12pxh"
                    android:layout_marginTop="@dimen/_14pxv" android:layout_width="@dimen/_33pxh">
                    <ImageView android:id="@+id/imagePowerButtonOne"
                        android:layout_gravity="start"
                        android:layout_height="@dimen/_28pxh"
                        android:layout_width="@dimen/_33pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_powerbutton1"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_powerbutton1"/>
                    <ImageView android:id="@+id/imagePowerButtonOneOne"
                        android:layout_gravity="start"
                        android:layout_height="@dimen/_28pxh"
                        android:layout_width="@dimen/_33pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_powerbutton1"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_powerbutton1"/>
                </FrameLayout>
                <ImageView android:id="@+id/imagePowerButtonOneTwo"
                    android:layout_gravity="center"
                    android:layout_height="@dimen/_28pxh"
                    android:layout_marginBottom="@dimen/_14pxv"
                    android:layout_marginEnd="@dimen/_12pxh"
                    android:layout_marginStart="@dimen/_11pxh"
                    android:layout_marginTop="@dimen/_14pxv"
                    android:layout_width="@dimen/_33pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_powerbutton1_28x33"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_powerbutton1_28x33"/>
            </FrameLayout>
        </LinearLayout>
        <FrameLayout android:id="@+id/frameBottomappbarclock"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_10pxh" android:layout_width="match_parent">
            <LinearLayout android:id="@+id/linearRow"
                android:layout_height="match_parent"
                android:layout_marginBottom="@dimen/_18pxv"
                android:layout_marginEnd="@dimen/_59pxh"
                android:layout_marginStart="@dimen/_7pxh"
                android:layout_width="match_parent" android:orientation="horizontal">
                <LinearLayout android:id="@+id/linearRowclock"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.59"
                    android:layout_width="0dp" android:orientation="horizontal">

                    <com.google.android.material.floatingactionbutton.FloatingActionButton
                        android:id="@+id/floatingBtnFloatingactionbutton"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.85" android:layout_width="0dp"/>
                    <ImageView android:id="@+id/imageClock"
                        android:layout_height="@dimen/_19pxh"
                        android:layout_marginBottom="@dimen/_10pxv"
                        android:layout_marginStart="@dimen/_14pxh"
                        android:layout_marginTop="@dimen/_28pxv"
                        android:layout_width="@dimen/_19pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_clock"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_clock"/>
                </LinearLayout>
                <LinearLayout android:id="@+id/linearRowvector"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_10pxv"
                    android:layout_marginStart="@dimen/_59pxh"
                    android:layout_marginTop="@dimen/_28pxv"
                    android:layout_weight="0.41"
                    android:layout_width="0dp" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageVector"
                        android:layout_height="@dimen/_19pxh"
                        android:layout_width="@dimen/_19pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_vector"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_vector"/>
                    <ImageView android:id="@+id/imageLock"
                        android:layout_height="@dimen/_18pxh"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginStart="@dimen/_59pxh"
                        android:layout_width="@dimen/_19pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_lock"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_lock"/>
                </LinearLayout>
            </LinearLayout>
        </FrameLayout>
    </LinearLayout>
</layout>
