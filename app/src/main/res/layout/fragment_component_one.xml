<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="componentOneVM" type="com.safiuddinsapplication.app.modules.componentone.data.viewmodel.ComponentOneVM"/>
    </data>
    <FrameLayout android:id="@+id/frameStacksubtract"
        android:layout_height="match_parent"
        android:layout_marginBottom="@dimen/_20pxv"
        android:layout_marginEnd="@dimen/_20pxh"
        android:layout_marginStart="@dimen/_19pxh"
        android:layout_marginTop="@dimen/_20pxv" android:layout_width="match_parent">
        <LinearLayout android:id="@+id/linearColumnsubtract"
            android:layout_gravity="start"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" android:orientation="vertical">
            <androidx.appcompat.widget.Toolbar
                android:gravity="center_horizontal"
                android:id="@+id/toolbarToolbar"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_1pxh"
                android:layout_width="match_parent" app:contentInsetStart="0dp">
                <FrameLayout android:id="@+id/frameProperty1Home"
                    android:layout_height="@dimen/_84pxv" android:layout_width="match_parent">
                    <LinearLayout android:id="@+id/linearRow"
                        android:layout_gravity="bottom|start"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_10pxv"
                        android:layout_width="match_parent"
                        android:orientation="horizontal" style="@style/groupStylegray_900cornerRadius">
                        <LinearLayout
                            android:id="@+id/linearRowsubtract"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_18pxv"
                            android:layout_marginStart="@dimen/_14pxh"
                            android:layout_weight="0.59"
                            android:layout_width="0dp" android:orientation="horizontal">
                            <ImageView android:id="@+id/imageSubtract"
                                android:layout_height="@dimen/_58pxh"
                                android:layout_width="@dimen/_110pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_subtract"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_subtract"/>
                            <ImageView android:id="@+id/imageClock"
                                android:layout_height="@dimen/_19pxh"
                                android:layout_marginBottom="@dimen/_10pxv"
                                android:layout_marginStart="@dimen/_14pxh"
                                android:layout_marginTop="@dimen/_28pxv"
                                android:layout_width="@dimen/_19pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_clock"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_clock"/>
                        </LinearLayout>
                        <LinearLayout android:id="@+id/linearRowvector"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_28pxv"
                            android:layout_marginEnd="@dimen/_59pxh"
                            android:layout_marginStart="@dimen/_59pxh"
                            android:layout_marginTop="@dimen/_28pxv"
                            android:layout_weight="0.41"
                            android:layout_width="0dp" android:orientation="horizontal">
                            <ImageView android:id="@+id/imageVector"
                                android:layout_height="@dimen/_19pxh"
                                android:layout_width="@dimen/_19pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_vector"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_vector"/>
                            <ImageView android:id="@+id/imageLock"
                                android:layout_height="@dimen/_18pxh"
                                android:layout_marginBottom="@dimen/_1pxv"
                                android:layout_marginStart="@dimen/_59pxh"
                                android:layout_width="@dimen/_19pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_lock"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_lock"/>
                        </LinearLayout>
                    </LinearLayout>
                    <FrameLayout android:id="@+id/frameHome"
                        android:layout_gravity="top|start"
                        android:layout_height="@dimen/_56pxv"
                        android:layout_marginBottom="@dimen/_10pxv"
                        android:layout_marginEnd="@dimen/_41pxh"
                        android:layout_marginStart="@dimen/_41pxh"
                        android:layout_width="@dimen/_56pxh" style="@style/groupStylelime_1">
                        <ImageView android:id="@+id/imagePowerButtonOne"
                            android:layout_gravity="center"
                            android:layout_height="@dimen/_28pxh"
                            android:layout_marginBottom="@dimen/_14pxv"
                            android:layout_marginEnd="@dimen/_11pxh"
                            android:layout_marginStart="@dimen/_12pxh"
                            android:layout_marginTop="@dimen/_14pxv"
                            android:layout_width="@dimen/_33pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_powerbutton1"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_powerbutton1"/>
                        <ImageView
                            android:id="@+id/imagePowerButtonOneOne"
                            android:layout_gravity="center"
                            android:layout_height="@dimen/_28pxh"
                            android:layout_marginBottom="@dimen/_14pxv"
                            android:layout_marginEnd="@dimen/_11pxh"
                            android:layout_marginStart="@dimen/_12pxh"
                            android:layout_marginTop="@dimen/_14pxv"
                            android:layout_width="@dimen/_33pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_powerbutton1"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_powerbutton1"/>
                    </FrameLayout>
                </FrameLayout>
            </androidx.appcompat.widget.Toolbar>
            <LinearLayout android:gravity="center_horizontal"
                android:id="@+id/linearColumn"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_38pxv"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStylewhite_A700">
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearColumnsubtractOne"
                    android:layout_height="match_parent"
                    android:layout_width="match_parent"
                    android:orientation="vertical" style="@style/groupStylegray_902cornerRadius">
                    <androidx.recyclerview.widget.RecyclerView
                        android:id="@+id/recyclerListsubtractOne"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:orientation="vertical"
                        app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                        itemSpace="@{@dimen/_38pxh}" tools:itemCount="2" tools:listitem="@layout/row_listsubtract_one"/>
                    <View android:id="@+id/viewProperty1Cloc"
                        android:layout_gravity="center_horizontal"
                        android:layout_height="@dimen/_84pxv"
                        android:layout_marginStart="@dimen/_1pxh"
                        android:layout_marginTop="@dimen/_38pxv" android:layout_width="@dimen/_375pxh"/>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>

        <com.google.android.material.floatingactionbutton.FloatingActionButton
            android:backgroundTint="@color/lime_500"
            android:id="@+id/floatingBtnFloatingactionbuttonOne"
            android:layout_gravity="bottom|end"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_28pxv"
            android:layout_marginEnd="@dimen/_41pxh"
            android:layout_marginStart="@dimen/_41pxh"
            android:layout_marginTop="@dimen/_28pxv"
            android:layout_width="wrap_content"
            app:fabCustomSize="@dimen/_56pxh"
            app:srcCompat="@drawable/img_lock_gray_900" app:tint="@color/gray_900"/>
    </FrameLayout>
</layout>
