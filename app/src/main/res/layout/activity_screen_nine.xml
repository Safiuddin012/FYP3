<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="screenNineVM" type="com.safiuddinsapplication.app.modules.screennine.data.viewmodel.ScreenNineVM"/>
    </data>
    <LinearLayout android:gravity="start"
        android:id="@+id/linearColumngroupone"
        android:layout_height="match_parent"
        android:layout_marginBottom="@dimen/_24pxv"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStyleblack_900">
        <LinearLayout android:id="@+id/linearScreenNine"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStyleblack_900">
            <FrameLayout android:gravity="start"
                android:id="@+id/frameStackgroupone"
                android:layout_height="@dimen/_175pxv" android:layout_width="match_parent">
                <ImageView android:id="@+id/imageGroupOne"
                    android:layout_gravity="start"
                    android:layout_height="@dimen/_175pxh"
                    android:layout_width="match_parent"
                    android:scaleType="fitXY"
                    imageRes="@{@drawable/img_group1}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_group1"/>
                <LinearLayout android:id="@+id/linearRowprofile"
                    android:layout_gravity="end"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_54pxv"
                    android:layout_marginEnd="@dimen/_52pxh"
                    android:layout_marginStart="@dimen/_52pxh"
                    android:layout_marginTop="@dimen/_54pxv"
                    android:layout_width="wrap_content" android:orientation="horizontal">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtProfile"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_23pxv"
                        android:layout_weight="0.71"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{screenNineVM.screenNineModel.txtProfile}"
                        style="@style/txtABeeZeeitalic32" tools:text="@string/lbl_profile"/>
                    <ImageView android:id="@+id/imageEllipseEightyTwo"
                        android:layout_height="@dimen/_40pxh"
                        android:layout_marginBottom="@dimen/_21pxv"
                        android:layout_marginStart="@dimen/_73pxh"
                        android:layout_width="@dimen/_40pxh"
                        android:scaleType="centerCrop"
                        circular="@{true}"
                        imageRes="@{@drawable/img_ellipse78}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_ellipse78"/>
                </LinearLayout>
            </FrameLayout>
            <FrameLayout android:id="@+id/frameEmail"
                android:layout_gravity="start"
                android:layout_height="@dimen/_37pxv"
                android:layout_marginEnd="@dimen/_28pxh"
                android:layout_marginStart="@dimen/_28pxh"
                android:layout_marginTop="@dimen/_111pxv"
                android:layout_width="@dimen/_318pxh" style="@style/groupStylecornerRadius_3">
                <ImageView android:id="@+id/imageIcoutlineemai"
                    android:layout_gravity="start"
                    android:layout_height="@dimen/_15pxh"
                    android:layout_marginBottom="@dimen/_11pxv"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginStart="@dimen/_8pxh"
                    android:layout_marginTop="@dimen/_11pxv"
                    android:layout_width="@dimen/_20pxh"
                    android:scaleType="centerCrop" circular="@{true}"
                    imageRes="@{@drawable/img_icoutlineemai}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_icoutlineemai"/>
                <androidx.appcompat.widget.AppCompatButton
                    android:ellipsize="end" android:gravity="center"
                    android:id="@+id/btnChangeEmail"
                    android:layout_gravity="start"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:paddingBottom="@dimen/_9pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_30pxh"
                    android:paddingTop="@dimen/_9pxv"
                    android:singleLine="true"
                    android:text="@string/lbl_change_email"
                    android:textAllCaps="false"
                    style="@style/btnRoundedOutline_2" tools:text="@string/lbl_change_email"/>
            </FrameLayout>
            <androidx.recyclerview.widget.RecyclerView
                android:gravity="start"
                android:id="@+id/recyclerScreenNine"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_28pxh"
                android:layout_marginStart="@dimen/_28pxh"
                android:layout_marginTop="@dimen/_29pxv"
                android:layout_width="match_parent"
                android:orientation="vertical"
                app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                itemSpace="@{@dimen/_29pxh}" tools:itemCount="2" tools:listitem="@layout/row_screen_nine"/>
            <androidx.appcompat.widget.AppCompatButton
                android:ellipsize="end" android:gravity="center"
                android:id="@+id/btnSave"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_19pxh"
                android:layout_marginStart="@dimen/_19pxh"
                android:layout_marginTop="@dimen/_132pxv"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_23pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingStart="@dimen/_30pxh"
                android:paddingTop="@dimen/_23pxv"
                android:singleLine="true"
                android:text="@string/lbl_save"
                android:textAllCaps="false"
                style="@style/btnSolidRounded_1" tools:text="@string/lbl_save"/>
            <FrameLayout android:id="@+id/frameStack"
                android:layout_gravity="start"
                android:layout_height="@dimen/_56pxv"
                android:layout_marginEnd="@dimen/_41pxh"
                android:layout_marginStart="@dimen/_41pxh"
                android:layout_marginTop="@dimen/_80pxv"
                android:layout_width="@dimen/_56pxh" style="@style/groupStylelime_1">
                <FrameLayout android:id="@+id/frameHome"
                    android:layout_gravity="center"
                    android:layout_height="@dimen/_28pxv"
                    android:layout_marginBottom="@dimen/_14pxv"
                    android:layout_marginEnd="@dimen/_11pxh"
                    android:layout_marginStart="@dimen/_12pxh"
                    android:layout_marginTop="@dimen/_14pxv" android:layout_width="@dimen/_33pxh">
                    <ImageView android:id="@+id/imagePowerButtonOne"
                        android:layout_gravity="start"
                        android:layout_height="@dimen/_28pxh"
                        android:layout_width="@dimen/_33pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_powerbutton1"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_powerbutton1"/>
                    <ImageView android:id="@+id/imagePowerButtonOneOne"
                        android:layout_gravity="start"
                        android:layout_height="@dimen/_28pxh"
                        android:layout_width="@dimen/_33pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_powerbutton1"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_powerbutton1"/>
                </FrameLayout>
                <ImageView android:id="@+id/imagePowerButtonOneTwo"
                    android:layout_gravity="center"
                    android:layout_height="@dimen/_28pxh"
                    android:layout_marginBottom="@dimen/_14pxv"
                    android:layout_marginEnd="@dimen/_12pxh"
                    android:layout_marginStart="@dimen/_11pxh"
                    android:layout_marginTop="@dimen/_14pxv"
                    android:layout_width="@dimen/_33pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_powerbutton1_28x33"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_powerbutton1_28x33"/>
            </FrameLayout>
        </LinearLayout>
        <FrameLayout android:gravity="center_horizontal"
            android:id="@+id/frameBottomappbarclock"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_15pxh" android:layout_width="match_parent">
            <LinearLayout android:id="@+id/linearRow"
                android:layout_height="match_parent"
                android:layout_marginBottom="@dimen/_18pxv"
                android:layout_marginEnd="@dimen/_59pxh"
                android:layout_marginStart="@dimen/_14pxh"
                android:layout_width="match_parent" android:orientation="horizontal">
                <LinearLayout android:id="@+id/linearRowclock"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.59"
                    android:layout_width="0dp" android:orientation="horizontal">

                    <com.google.android.material.floatingactionbutton.FloatingActionButton
                        android:id="@+id/floatingBtnFloatingactionbutton"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.85" android:layout_width="0dp"/>
                    <ImageView android:id="@+id/imageClock"
                        android:layout_height="@dimen/_19pxh"
                        android:layout_marginBottom="@dimen/_10pxv"
                        android:layout_marginStart="@dimen/_14pxh"
                        android:layout_marginTop="@dimen/_28pxv"
                        android:layout_width="@dimen/_19pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_clock"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_clock"/>
                </LinearLayout>
                <LinearLayout android:id="@+id/linearRowvector"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_10pxv"
                    android:layout_marginStart="@dimen/_59pxh"
                    android:layout_marginTop="@dimen/_28pxv"
                    android:layout_weight="0.41"
                    android:layout_width="0dp" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageVector"
                        android:layout_height="@dimen/_19pxh"
                        android:layout_width="@dimen/_19pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_vector"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_vector"/>
                    <ImageView android:id="@+id/imageLock"
                        android:layout_height="@dimen/_18pxh"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginStart="@dimen/_59pxh"
                        android:layout_width="@dimen/_19pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_lock"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_lock"/>
                </LinearLayout>
            </LinearLayout>
        </FrameLayout>
    </LinearLayout>
</layout>
