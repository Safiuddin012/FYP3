<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="dropdownOneVM" type="com.safiuddinsapplication.app.modules.dropdownone.data.viewmodel.DropdownOneVM"/>
    </data>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearDropdownOne"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <LinearLayout android:gravity="start"
            android:id="@+id/linearDropdownContai"
            android:layout_height="match_parent"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylecornerRadius_4">
            <LinearLayout android:gravity="center_horizontal"
                android:id="@+id/linearStateDefault"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_20pxh"
                android:layout_marginStart="@dimen/_20pxh"
                android:layout_marginTop="@dimen/_28pxv"
                android:layout_width="wrap_content"
                android:orientation="vertical" style="@style/groupStylecornerRadius_5">
                <Spinner
                    android:background="@drawable/layer_list_bg_white_a700_border_black_900_19_radius_8"
                    android:gravity="start"
                    android:id="@+id/spinnerButton"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" tools:listitem="@layout/spinner_item"/>
            </LinearLayout>
            <FrameLayout android:id="@+id/frameStateHover"
                android:layout_height="@dimen/_36pxv"
                android:layout_marginEnd="@dimen/_20pxh"
                android:layout_marginStart="@dimen/_20pxh"
                android:layout_marginTop="@dimen/_16pxv"
                android:layout_width="@dimen/_164pxh" style="@style/groupStylecornerRadius_6">
                <View
                    android:background="@drawable/rectangle_border_black_900_19_radius_8"
                    android:id="@+id/viewCreatefromfra"
                    android:layout_gravity="center"
                    android:layout_height="@dimen/_36pxv"
                    android:layout_marginEnd="@dimen/_1pxh" android:layout_width="@dimen/_163pxh"/>
                <Spinner
                    android:background="@drawable/layer_list_bg_gray_100_border_black_900_19_radius_8"
                    android:id="@+id/spinnerButtonOne"
                    android:layout_gravity="start"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" tools:listitem="@layout/spinner_item"/>
            </FrameLayout>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearStateOpen"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_20pxv"
                android:layout_marginEnd="@dimen/_20pxh"
                android:layout_marginStart="@dimen/_20pxh"
                android:layout_marginTop="@dimen/_16pxv"
                android:layout_width="wrap_content"
                android:orientation="vertical" style="@style/groupStylecornerRadius_5">
                <Spinner
                    android:background="@drawable/layer_list_bg_gray_100_border_black_900_19_radius_8"
                    android:id="@+id/spinnerButtonTwo"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" tools:listitem="@layout/spinner_item"/>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearListofitems"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_4pxv"
                    android:layout_width="match_parent"
                    android:orientation="vertical" style="@style/groupStylewhite_A700cornerRadius">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtDuration"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_16pxh"
                        android:layout_marginStart="@dimen/_16pxh"
                        android:layout_marginTop="@dimen/_15pxv"
                        android:layout_width="wrap_content"
                        android:lineSpacingExtra="@dimen/_4pxh"
                        android:singleLine="true"
                        android:text="@{dropdownOneVM.dropdownOneModel.txtDuration}"
                        style="@style/txtFiraSansregular16" tools:text="@string/lbl_30_seconds"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtDurationOne"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_16pxh"
                        android:layout_marginStart="@dimen/_16pxh"
                        android:layout_marginTop="@dimen/_20pxv"
                        android:layout_width="wrap_content"
                        android:lineSpacingExtra="@dimen/_4pxh"
                        android:singleLine="true"
                        android:text="@{dropdownOneVM.dropdownOneModel.txtDurationOne}"
                        style="@style/txtFiraSansregular16" tools:text="@string/lbl_60_seconds"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtDurationTwo"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_16pxv"
                        android:layout_marginEnd="@dimen/_16pxh"
                        android:layout_marginStart="@dimen/_16pxh"
                        android:layout_marginTop="@dimen/_20pxv"
                        android:layout_width="wrap_content"
                        android:lineSpacingExtra="@dimen/_4pxh"
                        android:singleLine="true"
                        android:text="@{dropdownOneVM.dropdownOneModel.txtDurationTwo}"
                        style="@style/txtFiraSansregular16" tools:text="@string/lbl_90_seconds"/>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</layout>
